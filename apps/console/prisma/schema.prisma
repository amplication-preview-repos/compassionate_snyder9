datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  created  DateTime @default(now())
  email    String?  @unique
  id       String   @id @default(cuid())
  modified DateTime @updatedAt
  password String
  roles    Json
  username String   @unique
}
